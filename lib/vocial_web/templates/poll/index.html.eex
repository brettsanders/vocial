<i id="enable-polls-channel" />

<button id="polls-ping" class="btn btn-primary">Ping Websocket</button>

<%= for poll <- @polls do %>
  <h2>Poll: <%= poll.title %></h2>
  <p>Total Options: <%= Enum.count(poll.options) %></p>

  <%= for option <- poll.options do %>
    <strong><%= option.title %></strong>: <%= option.votes %> votes
    <br />
  <% end %>
<% end %>
